PREFIX schema: <https://schema.org/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX schemas: <https://schema.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ludeme: <https://w3id.org/ludeme/>
PREFIX ludata: <https://w3id.org/ludeme/data/>
PREFIX dc: <http://purl.org/dc/terms/>

SELECT * WHERE {
  ?game a schemas:Game ;
  	rdfs:label ?label ;
  	schema:countryOfOrigin ?Origin ;
    schema:description ?image .
    BIND(strdt(?image, rdf:HTML) as ?imagetDescription)
    ?game foaf:depiction/foaf:thumbnail ?widgetImage.
   #FILTER contains(LCASE(?label), "senet")
} LIMIT 1000